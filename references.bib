@ARTICLE{Weinkove2008-nr,
  title    = "Fibrinogen concentrate for acquired hypofibrinogenaemic states",
  author   = "Weinkove, R and Rangarajan, S",
  abstract = "This study aims to assess the efficacy and safety of fibrinogen
