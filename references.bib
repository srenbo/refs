@ARTICLE{Butenas2013-bw,
  title    = "The nature of lipopolysaccharide-stimulated monocyte tissue
              factor activity",
  author   = "Butenas, Saulius and Gissel, Matthew and Krudysz-Amblo, Jolanta
              and Mann, Kenneth G",
  abstract = "There are several contradictory hypotheses that attempt to
              explain changes in cell tissue factor (TF) activity upon
