@ARTICLE{Wandt2012-rb,
  title     = "Therapeutic platelet transfusion versus routine prophylactic
               transfusion in patients with haematological malignancies: an
               open-label, multicentre, randomised study",
  author    = "Wandt, Hannes and Schaefer-Eckart, Kerstin and Wendelin, Knut
