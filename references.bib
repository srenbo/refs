@ARTICLE{Yang2014-uy,
  title    = "[Analysis of empirical treatment for newly diagnosed acute
              promyelocytic leukemia combined with disseminated intravascular
              coagulation]",
  author   = "Yang, Hua and Zhu, Cheng-Ying and Wang, Quan-Shun and Niu,
