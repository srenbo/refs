@ARTICLE{Justo_Sanz2019-xi,
  title    = "Platelet Apoptosis and {PAI-1} are Involved in the
              {Pro-Coagulant} State of Immune Thrombocytopaenia Patients
              Treated with Thrombopoietin Receptor Agonists",
  author   = "Justo Sanz, Raul and Monz{\'o}n Manzano, Elena and Fern{\'a}ndez
